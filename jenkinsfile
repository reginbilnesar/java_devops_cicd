@Library('my-shared-library') _ 
pipeline {
    agent any

    stages {

        stage('Git Checkout') {

            steps {
                
                script{
                    // Checkout the code from GitHub repository
                   gitCheckout(
                    branch: "main",
                    url: "https://github.com/reginbilnesar/java_devops_cicd.git"  
                   )
                }
                
            }
            
        }
        stage('Unit Test using maven') {
            steps {
                dir('/Desktop/devops/mrdevops_java_app-main') {  // Correct path to your project
                    mvnTest()
                }
            }
        }
    }
}
